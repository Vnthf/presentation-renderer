{
  "name": "markdown_renderer",
  "version": "1.0.0",
  "description": "markdown renderer for dooray-project",
  "main": "dist/presentation.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "dev": "webpack-dev-server --hot --inline",
    "test:check-node": "node test/node-test",
    "test": "jest --watchAll",
    "build": "webpack --mode production",
    "tui-plugin": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.nhnent.com/dooray/markdown-renderer.git"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-flow": "7.0.0-beta.40",
    "@babel/preset-stage-0": "7.0.0-beta.40",
    "@babel/register": "7.0.0-beta.40",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta.2",
    "concat-cli": "^4.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "get-json": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.99.1",
    "jest": "^23.1.0",
    "jest-serializer-html": "^5.0.0",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.8.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "shebang-loader": "0.0.1",
    "style-loader": "^0.21.0",
    "uglify-js": "^2.7.3",
    "uglifycss": "0.0.25",
    "url-loader": "^0.5.9",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\$tuiSource/(.*)$": "tui-editor-source/src/js/$1",
      "\\highlightjs/(.*)$": "highlightjs/$1",
      "\\prettyJson/(.*)$": "pretty-json-string/dist/$1",
      "\\$katex/(.*)$": "katex/dist/$1"
    }
  },
