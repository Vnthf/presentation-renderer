  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "emoji-js": "^3.4.0",
    "highlightjs": "^9.10.0",
    "juice": "^4.2.3",
    "katex": "^0.8.3",
    "lodash": "^4.17.5",
    "markdown-it": "^8.4.1",
    "markdown-it-for-inline": "^0.1.1",
    "nodelist-foreach-polyfill": "^1.2.0",
    "pretty-json-string": "^1.0.3",
    "to-mark": "^1.1.2",
    "tui-chart": "3.3.1",
    "tui-editor": "1.2.6",
    "tui-editor-source": "github:Vnthf/tui.editor#production"
